// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model ElectricityPrice {
  datetime     String
  datetime_utc String
  tz_time      String
  geo_id       Int
  geo_name     String
  value        Float

  @@id([datetime_utc, geo_id])
}
