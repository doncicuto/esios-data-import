{
  "name": "pvpc-to-postgres",
  "version": "1.0.0",
  "description": "Import PVPC electricity prices from ESIOS api and save them in postgres",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "start": "npm run build && node dist/index.js",
    "start:prod": "npx prisma migrate deploy && node dist/index.js",
    "test": "ts-node **/*.test.ts | tap-arc"
  },
  "keywords": [
    "ESIOS",
    "Electricity",
    "Price",
    "Spain",
    "PVPC"
  ],
  "author": "Miguel Cabrerizo",
  "license": "MIT",
  "devDependencies": {
    "@types/luxon": "^2.3.1",
    "@types/node": "^17.0.31",
    "@types/node-fetch": "^2.6.1",
    "@types/tape": "^4.13.2",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "eslint": "^8.14.0",
    "prisma": "^3.13.0",
    "tap-arc": "^0.3.4",
    "tape": "^5.5.3",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "@prisma/client": "^3.13.0",
    "luxon": "^2.3.2",
    "node-fetch": "^2.6.7"
  }
}
